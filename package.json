{
  "name": "ui-kit-react-tailwind",
  "author": "Dave Hudson",
  "version": "0.1.1-2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "css/uikit.css",
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prestart": "npm run build-tailwind",
    "start": "tsdx watch",
    "prebuild": "npm run build-tailwind",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-tailwind": "npx tailwindcss-cli build -i ./tailwind.css -o ./css/uikit.css"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine": "auto"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "module": "dist/ui-kit.esm.js",
  "size-limit": [
    {
      "path": "dist/ui-kit.cjs.production.min.js",
      "limit": "50 KB"
    },
    {
      "path": "dist/ui-kit.esm.js",
      "limit": "50 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^1.0.5",
    "@size-limit/preset-small-lib": "^7.0.5",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/addons": "^6.4.14",
    "@storybook/react": "^6.4.14",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-table": "^7.7.9",
    "@types/tailwindcss": "^3.0.3",
    "babel-loader": "^8.2.3",
    "husky": "^7.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "rollup-plugin-static-files": "^0.2.0",
    "size-limit": "^7.0.5",
    "tailwindcss": "^3.0.17",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "dayjs": "^1.10.7",
    "prettier": "^2.5.1",
    "react-table": "^7.7.0"
  }
}
